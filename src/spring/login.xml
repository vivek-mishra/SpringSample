<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN/EN" "http://ww.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="login" class="spring.LoginUser" >
		<constructor-arg index="0"><value>sprig</value></constructor-arg>
		<constructor-arg index="1"><value>spring</value></constructor-arg>
	</bean>
	
	<bean id="dao" class="spring.loginDB" >
		<property name="dbms"><value>mysql</value></property>
		<property name="url"><value>jdbc:mysql://localhost:3306/</value></property>
		<property name="driver"><value>com.mysql.jdbc.Driver</value></property>
		<property name="dbName"><value>prepaid2</value></property>
		<property name="userName"><value>prepaid2</value></property>
		<property name="password"><value>prepaid2</value></property>
	</bean>
</beans>